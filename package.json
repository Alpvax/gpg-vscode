{
    "name": "gpg-syntax",
    "displayName": "GasPoweredDSSyntax",
    "description": "Syntax hightlighting for .gas and .skrit files",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "gpg-gas",
            "aliases": ["gas", "gpg-gas"],
            "extensions": [".gas",".skrit"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "gpg-gas",
            "scopeName": "source.gas",
            "path": "./syntaxes/gpg-gas.tmLanguage.json"
        }]
    }
}