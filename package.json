{
  "name": "gpg-syntax",
  "displayName": "GasPoweredDSSyntax",
  "description": "Syntax hightlighting for .gas and .skrit files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.29.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "gpg-skrit",
        "aliases": ["skrit", "gpg-skrit"],
        "extensions": [".skrit"],
        "configuration": "./language-config/skrit.json"
      },
      {
        "id": "gpg-gas",
        "aliases": ["gas", "gpg-gas"],
        "extensions": [".gas"],
        "configuration": "./language-config/gas.json"
      }
    ],
    "grammars": [
      {
        "language": "gpg-skrit",
        "scopeName": "source.gpg-skrit",
        "path": "./syntaxes/gpg-skrit.tmLanguage.json"
      },
      {
        "language": "gpg-gas",
        "scopeName": "source.gpg-gas",
        "path": "./syntaxes/gpg-gas.tmLanguage.json"
      }
    ]
  }
}
